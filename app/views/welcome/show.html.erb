<div>
<h1>Colorado Graduation Rates</h1>
</div>
<div class-"container">
<div class ="row">
<div class="input-field col s6" id="search">
    <%= form_tag :search, method: :get do%>
    <%= text_field_tag :college, "Name" %>
    <%= collection_select :program, :id, Program.all, :id, :name %>
    <%= text_field_tag :degree, "Degree" %>
    <%= text_field_tag :ethnicity, "Ethnicity" %>
    <%= text_field_tag :year, "Year" %>
    <%= submit_tag "Get Data"%>
  <% end %>
</div>
</div>
</div>

<div class="container">
 <div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <input id="school_name" type="text" class="validate">
          <label for="school_name"> School Name</label>
        </div>
        <div class="input-field col s6">
          <input id="program_name" type="text" class="validate">
          <label for="program_name">Program</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <input id="ethnicity" type="text" class="validate">
          <label for="ethncitiy">Ethnicity</label>
        </div>
        <div class="input-field col s6">
          <input id="year" type="text" class="validate">
          <label for="year">Year</label>
        </div>
      </div>
      <div class="row">
        <div class="col s12">
          This is an inline input field:
          <div class="input-field inline">
            <input id="email" type="email" class="validate">
            <label for="email" data-error="wrong" data-success="right">Email</label>
          </div>
        </div>
      </div>
    </form>
  </div>
  </div>